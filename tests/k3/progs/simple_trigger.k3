declare events : {|(int,int)|}

trigger on_event(a:int, b:int) {} = do {
  insert(events, a, b)
}

role client {
  source  s1 : (int, int) = stream([(1,2)])
  bind    s1 -> on_event
  consume s1
}

default role client

// Uncomment when running in -test -prog mode
expected 

events = {| (1,2); (3,4); (5,6) |}
